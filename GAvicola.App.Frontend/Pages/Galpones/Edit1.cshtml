@page
@using GAvicola.App.Dominio
@model GAvicola.App.Frontend.Pages.Galpones.Edit1Model
@{
}
<h1>Editar Galpon</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="galpon.Id"></label>
        <input asp-for="galpon.Id" class="form-control" readonly>
        <span asp-validation-for="galpon.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.Nombre"></label>
        <input asp-for="galpon.Nombre" class="form-control">
        <span asp-validation-for="galpon.Nombre" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.NumAves"></label>
        <input asp-for="galpon.NumAves" class="form-control">
        <span asp-validation-for="galpon.NumAves" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.FechaIngreso"></label>
        <input asp-for="galpon.FechaIngreso" class="form-control">
        <span asp-validation-for="galpon.FechaIngreso" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.FechaEgreso"></label>
        <input asp-for="galpon.FechaEgreso" class="form-control">
        <span asp-validation-for="galpon.FechaEgreso" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.Latitud"></label>
        <input asp-for="galpon.Latitud" class="form-control">
        <span asp-validation-for="galpon.Latitud" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="galpon.Longitud"></label>
        <input asp-for="galpon.Longitud" class="form-control">
        <span asp-validation-for="galpon.Longitud" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Modificar Galpon</button>



</form>
<A class="btn btn-secondary" asp-page="./index1">Volver</a>
