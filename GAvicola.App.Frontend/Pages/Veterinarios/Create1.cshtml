@page
@using GAvicola.App.Dominio
@model GAvicola.App.Frontend.Pages.Veterinarios.Create1Model
@{
}
<h1>Crear Veterinario</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="veterinario.Id"></label>
        <input asp-for="veterinario.Id" class="form-control" readonly>
        <span asp-validation-for="veterinario.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Nombre"></label>
        <input asp-for="veterinario.Nombre" class="form-control">
        <span asp-validation-for="veterinario.Nombre" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Documento"></label>
        <input asp-for="veterinario.Documento" class="form-control">
        <span asp-validation-for="veterinario.Documento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Usuario"></label>
        <input asp-for="veterinario.Usuario" class="form-control">
        <span asp-validation-for="veterinario.Usuario" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Clave"></label>
        <input asp-for="veterinario.Clave" class="form-control">
        <span asp-validation-for="veterinario.Clave" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Telefono"></label>
        <input asp-for="veterinario.Telefono" class="form-control">
        <span asp-validation-for="veterinario.Telefono" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Direccion"></label>
        <input asp-for="veterinario.Direccion" class="form-control">
        <span asp-validation-for="veterinario.Direccion" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="veterinario.Correo"></label>
        <input asp-for="veterinario.Correo" class="form-control">
        <span asp-validation-for="veterinario.Correo" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear Veterinario</button>


</form>
<A class="btn btn-secondary" asp-page="./index1">Volver</a>
